#include <iostream>
using namespace std;

void pattern1()
{
    int n;
    cin >> n;

    for (int row = 1; row <= n; row++) //NUMBER OF ROWS
    {
        for (int col = 1; col <= row; col++) // NUMBER OF COLS
        {
            cout << "* ";
        }
        cout<<""<< endl;
    }
}

int main()
{

    pattern1();

    return 0;
}
